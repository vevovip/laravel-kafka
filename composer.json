{
  "name": "yuriykim/laravel-kafka",
  "description": "A kafka driver for laravel",
  "type": "library",
  "require": {
    "php": "^8.0",
    "mateusjunges/laravel-kafka": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chocofamilyme\\LaravelKafka\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "yuriy kim",
      "email": "kim.yu@rahmetapp.kz",
      "role": "Backend Software Engineer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit tests",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes --config=.php_cs.dist.php"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chocofamilyme\\LaravelKafka\\Providers\\LaravelKafkaServiceProvider"
      ]
    }
  }
}
