{
  "name": "chocofamilyme/laravel-kafka",
  "description": "Laravel kafka wrapper with batch produce and consume",
  "keywords": [
    "laravel",
    "kafka",
    "brokers"
  ],
  "type": "library",
  "require": {
    "php": "^8.0",
    "mateusjunges/laravel-kafka": "^1.6"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chocofamilyme\\LaravelKafka\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "IT Chocolife.me",
      "homepage": "https://chocolife.me"
    }
  ],
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes --config=.php_cs.dist.php"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chocofamilyme\\LaravelKafka\\Providers\\LaravelKafkaServiceProvider"
      ]
    }
  }
}
